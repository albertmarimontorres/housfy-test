{
  "name": "app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:basic": "playwright test --config=playwright.config.no-server.ts",
    "test:e2e:auth": "playwright test --config=playwright.config.with-server.ts",
    "test:e2e:no-server": "pnpm test:e2e:basic",
    "test:e2e:with-server": "pnpm test:e2e:auth"
  },
  "dependencies": {
    "@mdi/font": "^7.4.47",
    "axios": "^1.13.2",
    "pinia": "^3.0.4",
    "vite-plugin-vuetify": "^2.1.2",
    "vue": "^3.5.24",
    "vue-router": "^4.6.3",
    "vuetify": "^3"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/coverage-v8": "^4.0.14",
    "@vitest/ui": "^4.0.14",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "happy-dom": "^20.0.11",
    "jsdom": "^27.2.0",
    "sass": "^1.94.2",
    "typescript": "~5.9.3",
    "vite": "^7.2.4",
    "vitest": "^4.0.14",
    "vue-tsc": "^3.1.4"
  }
}
